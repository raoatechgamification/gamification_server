{
  "name": "gamification",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "webpack --env APP_ENV=local --mode=development --progress",
    "build:dev": "webpack --env APP_ENV=development --mode=development --progress",
    "build:prod": "webpack --env APP_ENV=production --mode=production --progress",
    "start": "nodemon src/server.ts",
    "dev": "npx ts-node-dev --max-old-space-size=4096 src/server.ts",
    "compile": "npx tsc && node dist/server.js",
    "stage": "node ./dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "commit": "cz",
    "prod": "tsc && npm run copy-templates",
    "copy-templates": "copyfiles -u 1 src/templates/**/* dist"
  },
